.commissioned-page{
    padding-top: 25rpx;
}
/*姓名*/
.commissioned-page .name-wrap{
    padding-left: 25rpx;
    background-color: #fff;
}
.commissioned-page .name{
    background-color: #fff;
    display:flex;
    align-items: center;
    border-bottom:1px solid #eeeeee;
}
.commissioned-page .name .text{
    color: #555555;
    font-size: 34rpx;
    flex-shrink: 0;
}
.commissioned-page .name input{
    display:block;
    flex-grow: 1;
    padding: 0 25rpx 0 185rpx ;
    width: 100%;
    color:#333;
    font-size: 32rpx;
}
/*手机*/
.commissioned-page .mobile-wrap{
    padding-left: 25rpx;
    background-color: #fff;
}
.commissioned-page .mobile{
    background-color:#fff;
    display:flex;
    align-items: center;
    /*border-bottom:1px solid #bebebe;*/
}
.commissioned-page .mobile .text{
    color: #555555;
    font-size: 34rpx;
    flex-shrink: 0;
}
.commissioned-page .mobile input{
    display:block;
    flex-grow: 1;
    padding: 0 25rpx 0 151rpx ;
    width: 100%;
    color:#333;
    font-size: 32rpx;
}
/*区域,楼盘名称,面积,租金*/
.area-wrap{
    padding-left: 25rpx;
    background-color: #fff;
    margin-top:25rpx;
}
.estate-name-wrap,.size-wrap,.rent-wrap{
    padding-left: 25rpx;
    background-color: #fff;
}
.area,.estate-name,.size,.rent{
    display:flex;
    align-items: center;
    border-bottom:1px solid #eeeeee;
}
.area .arrow{
    width: 16rpx;
    height: 27rpx;
    padding-right: 30rpx;
}
.area .arrow image{
    width: 100%;
    height: 100%;
    display:block;
}
.rent{
    border-bottom:none;
}
.text{
    color: #555555;
    font-size: 34rpx;
    flex-shrink: 0;
}
.pseudo-input{
    display:block;
    flex-grow: 1;
    color:#333;
    font-size: 28rpx;
    padding: 0 25rpx 0 185rpx;
    height: 80rpx;
    line-height: 80rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
input{
    display:block;
    flex-grow: 1;
    width: 100%;
    color:#333;
    font-size: 28rpx;
    height: 80rpx;
    line-height: 80rpx;
}
.area input{
    padding: 0 25rpx 0 185rpx;
}
.estate-name input{
    padding: 0 25rpx 0 117rpx;
}
.size input{
    padding: 0 25rpx 0 124rpx;
}
.rent input{
    padding: 0 25rpx 0 56rpx;
}
/*备注*/
.remarks,.pseudo-textarea{
    margin-top: 25rpx;
    background-color: #fff;
    padding: 20rpx 25rpx;
}
.remarks .title,.pseudo-textarea .title{
    font-size: 29rpx;
    color: #555555;
    padding-bottom: 15rpx;
}
.pseudo-textarea .textarea{
    height: 240rpx;
}
.remarks textarea ,.pseudo-textarea .content{
    height: 240rpx;
    word-wrap: break-word;
    word-break: break-all;
    width: 100%;
    font-size: 28rpx;
    color: #333;
}
.placeholder{
    color: #a8a8a8;
    font-size: 28rpx;
}
/*按钮*/
button{
    margin: 50rpx 25rpx;
    background-color: #344966;
    color:#fff;
}
.button-hover{
    opacity: 0.7;
    background-color:#344966;
    color:#fff;
}
button[disabled][type="default"],button[disabled]:not([type]){
    color:#fff;
    background-color:#c5c5c5;
}